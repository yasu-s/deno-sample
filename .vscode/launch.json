{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno: debug",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--inspect-brk", "-A", "main.ts"],
      "port": 9229
    },
    {
      "name": "Deno: test",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["test", "--inspect-brk", "test.ts"],
      "port": 9229
    }
  ]
}
